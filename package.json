{
  "name": "stratagem-hero",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build --base=/stratagem-hero",
    "lint": "eslint --ext .ts,tsx,js,json --ignore-path .gitignore .",
    "preview": "vite preview --base=/stratagem-hero",
    "prepare": "husky",
    "format": "prettier -w -u .",
    "pre-commit": "npm run lint; npm run format; npm run test",
    "test": ""
  },
  "eslintConfig": {
    "extends": "preact"
  },
  "lint-staged": {
    "*.{js,ts,tsx,json}": "eslint",
    "*": "prettier -w -u"
  },
  "dependencies": {
    "preact": "^10.19.6"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.8.2",
    "@typescript-eslint/parser": "^7.9.0",
    "eslint": "^8.57.0",
    "eslint-config-preact": "^1.4.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.9.0",
    "vite": "^5.2.0",
    "vite-tsconfig-paths": "^4.3.2"
  }
}
